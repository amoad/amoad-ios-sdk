platform :ios, '11.0'
target 'AMoAdSdkDemo' do
  use_frameworks!
  pod "AMoAd"
  pod 'SnapKit'
  pod 'ObjectMapper'
  pod 'ReachabilitySwift'
end

post_install do |pi|
    pi.pods_project.targets.each do |t|
      t.build_configurations.each do |config|
        config.build_settings['IPHONEOS_DEPLOYMENT_TARGET'] = '11.0'
      end
    end
end

